<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Calculator">

    <Button
        android:id="@+id/Num2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="2"
        app:layout_constraintStart_toEndOf="@+id/Num1"
        app:layout_constraintTop_toTopOf="@+id/Num1" />

    <Button
        android:id="@+id/Num1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Num4" />

    <Button
        android:id="@+id/Num3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="3"
        app:layout_constraintStart_toEndOf="@+id/Num2"
        app:layout_constraintTop_toTopOf="@+id/Num2" />

    <Button
        android:id="@+id/Num5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="5"
        app:layout_constraintStart_toEndOf="@+id/Num4"
        app:layout_constraintTop_toTopOf="@+id/Num4" />

    <Button
        android:id="@+id/Num6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="6"
        app:layout_constraintStart_toEndOf="@+id/Num5"
        app:layout_constraintTop_toTopOf="@+id/Num5" />

    <Button
        android:id="@+id/Num4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Num7" />

    <Button
        android:id="@+id/b2_To_Pinven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inventory"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/b2_To_Calc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="Calc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/b2_To_Pinven" />

    <Button
        android:id="@+id/b2_To_Notes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="Notes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/b2_To_Calc" />

    <Button
        android:id="@+id/b2_To_Archive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="Archive"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/b2_To_Notes" />

    <EditText
        android:id="@+id/Button_Display"
        android:layout_width="346dp"
        android:layout_height="51dp"
        android:layout_marginTop="75dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Num7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="300dp"
        android:text="7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Num8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="8"
        app:layout_constraintStart_toEndOf="@+id/Num7"
        app:layout_constraintTop_toTopOf="@+id/Num7" />

    <Button
        android:id="@+id/Num9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="9"
        app:layout_constraintStart_toEndOf="@+id/Num8"
        app:layout_constraintTop_toTopOf="@+id/Num8" />

    <Button
        android:id="@+id/Num0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="0"
        app:layout_constraintEnd_toEndOf="@+id/Num2"
        app:layout_constraintStart_toEndOf="@+id/NumDec"
        app:layout_constraintTop_toBottomOf="@+id/Num2" />

    <Button
        android:id="@+id/NumDec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="."
        app:layout_constraintEnd_toEndOf="@+id/Num1"
        app:layout_constraintStart_toStartOf="@+id/Num1"
        app:layout_constraintTop_toBottomOf="@+id/Num1" />

    <Button
        android:id="@+id/NumEqual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/Num0"
        app:layout_constraintTop_toBottomOf="@+id/Num2" />

    <Button
        android:id="@+id/button_Multi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="*"
        app:layout_constraintStart_toEndOf="@+id/Num9"
        app:layout_constraintTop_toTopOf="@+id/Num9" />

    <Button
        android:id="@+id/button_Div"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        app:layout_constraintEnd_toEndOf="@+id/button_Multi"
        app:layout_constraintStart_toStartOf="@+id/button_Multi"
        app:layout_constraintTop_toBottomOf="@+id/button_Multi" />

    <Button
        android:id="@+id/button_Sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        app:layout_constraintEnd_toEndOf="@+id/button_Div"
        app:layout_constraintStart_toStartOf="@+id/button_Div"
        app:layout_constraintTop_toBottomOf="@+id/button_Div" />

    <Button
        android:id="@+id/button_Add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        app:layout_constraintEnd_toEndOf="@+id/button_Sub"
        app:layout_constraintStart_toStartOf="@+id/button_Sub"
        app:layout_constraintTop_toBottomOf="@+id/button_Sub" />

    <Button
        android:id="@+id/button_Open_Para"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:text="("
        app:layout_constraintBottom_toTopOf="@+id/Num9"
        app:layout_constraintEnd_toStartOf="@+id/button_Closed_Para" />

    <Button
        android:id="@+id/button_Closed_Para"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=")"
        app:layout_constraintBottom_toTopOf="@+id/button_Multi"
        app:layout_constraintEnd_toEndOf="@+id/button_Multi"
        app:layout_constraintStart_toStartOf="@+id/button_Multi" />

</androidx.constraintlayout.widget.ConstraintLayout>